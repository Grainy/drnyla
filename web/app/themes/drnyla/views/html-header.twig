<!--[if gt IE 8]><!--><html class="no-js" {{site.language_attributes}}> <!--<![endif]-->
<head>
    <meta charset="{{site.charset}}" />

    <meta name="description" content="{{site.description}}">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">


    <link rel="pingback" href="{{site.pingback_url}}" />
    <link rel="shortcut icon" href="{{site.theme.link}}/app/img/icon/favicon.png?v=2" type="image/x-icon" />

    <!-- FB share data -->
    {% if post.thumbnail %}
		{% set shareImg = post.thumbnail %}
	{% else %}
		{% set shareImg = site.theme.link~'/app/img/logo/logo@2x.png' %}
    {% endif %}
    <meta property="fb:app_id" content="1773131426233242" />
    <meta property="og:image" content="{{shareImg}}"/>

    <style type="text/css">
        .wf-loading .b-page-title {
          visibility: hidden;
        }
    </style>

    <script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?libraries=places&key=AIzaSyAbcGj5CwaGzsfbRmc6TFhVpwNqAwVYCD4"></script>

	<script src="https://use.typekit.net/hnv1wfv.js"></script>
	<script>try{Typekit.load({ async: true });}catch(e){}</script>

    {{function('wp_head')}}

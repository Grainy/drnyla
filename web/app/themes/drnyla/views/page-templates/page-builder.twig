{% extends "base.twig" %}

{% block content %}
	{% set hero = false %}

	{% for media_item in post.get_field('fc_page_sections') %}
		{% if loop.first %}
			{% set padding = '120px' %}

			{% else %}

			{% set padding = '50px' %}
		{% endif %}

		{% if media_item.acf_fc_layout == 'section_header' %}

			{% include 'layout/acf/section-header.twig' %}

		{% elseif media_item.acf_fc_layout == 'secondary_header' %}

			{% include 'layout/acf/secondary-header.twig' %}

		{% elseif media_item.acf_fc_layout == 'hero' %}

			{% include 'layout/acf/hero.twig' %}

		{% elseif media_item.acf_fc_layout == 'secondary_slider' %}

			{% include 'layout/acf/secondary-slider.twig' %}

		{% elseif media_item.acf_fc_layout == 'two_column_image' %}

			{% include 'layout/acf/two-column-image.twig' %}

		{% elseif media_item.acf_fc_layout == 'two_column_video' %}

			{% include 'layout/acf/two-column-video.twig' %}

		{% elseif media_item.acf_fc_layout == 'two_column_form' %}

			{% include 'layout/acf/two-column-form.twig' %}

		{% elseif media_item.acf_fc_layout == 'single_column_form' %}

			{% include 'layout/acf/single-column-form.twig' %}

		{% elseif media_item.acf_fc_layout == 'icon_ctas' %}

			{% include 'layout/acf/icon-ctas.twig' %}

		{% elseif media_item.acf_fc_layout == 'two_column_call_to_action' %}

			{% include 'layout/acf/calls-to-action.twig' %}

		{% elseif media_item.acf_fc_layout == 'secondary_nav' %}

			{% include 'layout/acf/secondary-nav.twig' %}

		{% elseif media_item.acf_fc_layout == 'two_column_text' %}

			{% include 'layout/acf/two-column-text.twig' %}

		{% elseif media_item.acf_fc_layout == 'brand_grid' %}

			{% include 'layout/acf/brand-grid.twig' %}

		{% elseif media_item.acf_fc_layout == 'breadcrumb_nav' %}

			{% include 'layout/acf/breadcrumb-nav.twig' %}

		{% elseif media_item.acf_fc_layout == 'latest_articles' %}

			{% include 'layout/acf/latest-articles.twig' %}

		{% elseif media_item.acf_fc_layout == 'latest_articles_category' %}

			{% include 'layout/acf/latest-articles-category.twig' %}

		{% elseif media_item.acf_fc_layout == 'products_grid' %}

			{% include 'layout/acf/products-grid.twig' %}

		{% elseif media_item.acf_fc_layout == 'faq_section' %}

			{% include 'layout/acf/faqs.twig' %}

		{% elseif media_item.acf_fc_layout == 'testimonial_block' %}

			{% include 'layout/acf/testimonial-block.twig' %}

		{% elseif media_item.acf_fc_layout == 'image_text_button' %}

			{% include 'layout/acf/image-text-button.twig' %}

		{% elseif media_item.acf_fc_layout == 'faq_block' %}

			{% include 'layout/acf/faq-block.twig' %}

		{% elseif media_item.acf_fc_layout == 'one_column_text' %}

			{% include 'layout/acf/one-column-text.twig' %}

		{% elseif media_item.acf_fc_layout == 'single_testimonial' %}

			{% include 'layout/acf/single-testimonial.twig' %}

		{% elseif media_item.acf_fc_layout == 'boxed_icon_ctas' %}

			{% include 'layout/acf/boxed-icon-ctas.twig' %}

		{% elseif media_item.acf_fc_layout == 'large_cta' %}

			{% include 'layout/acf/large-cta.twig' %}

		{% elseif media_item.acf_fc_layout == 'social_cta' %}

			{% include 'layout/acf/social-cta.twig' %}

		{% elseif media_item.acf_fc_layout == 'instagram_block' %}

			{% include 'layout/acf/instagram-block.twig' %}

		{% elseif media_item.acf_fc_layout == 'before_and_after' %}

			{% include 'layout/acf/before-and-after.twig' %}

		{% endif %}

		{% if media_item.acf_fc_layout == 'hero' %}
			{% set hero = true %}

			{% else %}

			{% set hero = false %}
		{% endif %}

	{% endfor %}

	{% include 'footer.twig' with {'pullup': '120'} %}
{% endblock %}

